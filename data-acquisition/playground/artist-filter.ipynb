   "source": [
    "const splitSongTitle = (title: string) => {\n",
    "  const match = title.match(/^([^({\\[]+)([({\\[].*)?$/);\n",
    "\n",
    "  return {\n",
    "    title: match?.[1]?.trim() ?? title,\n",
    "    extras: match?.[2]?.trim() ?? null,\n",
    "  };\n",
    "};\n",
    "\n",
    "splitSongTitle(\"sza  (featuring)[]{}\")\n",
    "\n",
    "\n",
    "\n"
   ]
